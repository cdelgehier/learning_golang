{
    "swagger": "2.0",
    "info": {
        "description": "This is the MyApp API server.",
        "title": "MyApp API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "DELGEHIER Cedric",
            "url": "https://twitter.com/JackNemrod"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/api/v1/products": {
            "get": {
                "description": "Get a list of all products known",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "products"
                ],
                "summary": "List products",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/models.Product"
                            }
                        }
                    }
                }
            }
        },
        "/ping": {
            "get": {
                "description": "Ping method for API Server",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Ping",
                "responses": {
                    "200": {
                        "description": "ping !",
                        "schema": {
                            "$ref": "#/definitions/models.Ping"
                        }
                    }
                }
            }
        },
        "/version": {
            "get": {
                "description": "Version of API-server",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Version",
                "responses": {
                    "200": {
                        "description": "version",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Ping": {
            "type": "object",
            "properties": {
                "pong": {
                    "type": "string",
                    "example": "2021-10-31 16:13:58.292387 +0000 UTC"
                }
            }
        },
        "models.Product": {
            "type": "object",
            "properties": {
                "brand": {
                    "type": "string",
                    "example": "Haribo"
                },
                "id": {
                    "type": "string",
                    "example": "1"
                },
                "name": {
                    "type": "string",
                    "example": "Rotella"
                },
                "price": {
                    "type": "number",
                    "example": 13.9
                }
            }
        }
    }
}